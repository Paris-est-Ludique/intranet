{
  "name": "intranet",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "packageManager": "yarn@4.0.1",
  "description": "Intranet for Paris est Ludique",
  "author": "Paris est Ludique <forceoranj@gmail.com> (https://github.com/forceoranj)",
  "license": "MIT",
  "homepage": "https://github.com/forceoranj/intranet",
  "repository": "https://github.com/forceoranj/intranet",
  "bugs": "https://github.com/forceoranj/intranet/issues",
  "keywords": [
    "react",
    "react-hooks",
    "redux",
    "redux-toolkit",
    "react-router",
    "express",
    "vite",
    "typescript",
    "postcss",
    "vitest",
    "unit-testing",
    "eslint",
    "stylelint",
    "prettier"
  ],
  "engines": {
    "node": ">=21.1.0",
    "npm": ">=10.2.0"
  },
  "scripts": {
    "dev": "vite-node server.ts",
    "build": "rimraf dist && tsc -p tsconfig.json && yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.ts",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "serve": "NODE_ENV=production vite-node server.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint --fix \"**/*.{css,scss,sass,ts,tsx}\"",
    "lint:format": "prettier -w . -u --loglevel silent",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "discord-register": "cross-env REGISTER_DISCORD_COMMANDS=true yarn build && yarn prod"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.7",
    "@sendgrid/mail": "7.7.0",
    "axios": "0.21.4",
    "bcrypt": "5.1.1",
    "chalk": "4.1.2",
    "classnames": "2.3.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "discord.js": "14.13.0",
    "express": "4.18.2",
    "google-auth-library": "9.2.0",
    "google-spreadsheet": "4.1.1",
    "googleapis": "88.2.0",
    "helmet": "4.6.0",
    "history": "4.10.1",
    "hpp": "0.2.3",
    "js-cookie": "3.0.5",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-modal": "3.16.1",
    "react-redux": "8.1.3",
    "react-router": "6.17.0",
    "react-router-dom": "6.17.0",
    "react-toastify": "9.1.3",
    "redux-devtools-extension": "2.13.9",
    "redux-first-history": "5.1.1",
    "redux-mock-store": "1.5.4",
    "redux-thunk": "2.4.2",
    "serialize-javascript": "6.0.1",
    "serve-static": "1.15.0",
    "web-push": "3.6.6",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.29",
    "@testing-library/react": "14.0.0",
    "@types/bcrypt": "5.0.1",
    "@types/compression": "1.7.4",
    "@types/cookie-parser": "1.4.5",
    "@types/express": "4.17.20",
    "@types/google-spreadsheet": "3.3.2",
    "@types/history": "5.0.0",
    "@types/hpp": "0.2.4",
    "@types/js-cookie": "3.0.5",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.14.200",
    "@types/morgan": "1.9.7",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/react-helmet": "6.1.8",
    "@types/react-modal": "3.16.2",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "18.0.5",
    "@types/redux-mock-store": "1.0.5",
    "@types/resolve": "1.20.5",
    "@types/serialize-javascript": "5.0.3",
    "@types/serve-static": "1.15.4",
    "@types/serviceworker": "0.0.76",
    "@types/web-push": "3.6.2",
    "@types/xml2js": "0.4.13",
    "@vitejs/plugin-react": "4.1.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "22.1.0",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "resolve": "1.22.8",
    "sass": "1.69.5",
    "stylelint": "15.11.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-sass-guidelines": "10.0.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.2.2",
    "unplugin-auto-import": "0.16.7",
    "unplugin-lightningcss": "0.1.0",
    "vite": "4.5.0",
    "vite-node": "0.34.6",
    "vitest": "0.34.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{css,sass,ts,tsx}": "stylelint --fix",
    "**/*": "prettier -w -u"
  }
}
